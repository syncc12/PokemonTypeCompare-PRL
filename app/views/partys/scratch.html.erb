        <!-- Search Pokemon Start -->
        <div class="row">
          <input id="input-pokemon" type="text" placeholder="Search Pokemon..." />
        </div>
        <!-- Search Pokemon End -->
        <div class="row">
          <!-- Pokemon Box Start -->
          <div id="moves-poke-box"><ol id="moves-poke-box-ol" class="list-unstyled"></ol></div>
          <!-- Pokemon Box End -->
          <!-- Add To Party Box Start -->
          <div id="party-box-hold"></div>
          <button disabled id="add-party-button" class="poke-button" type="button">Add To Party</button>
          <!-- Add To Party Box End -->
        </div>
      </div>
    </div>
    <!-- Pokemon Column End -->

    <!-- Move Column Start -->
    <div class="col-2 offset-1">
      <div class="move-column">
        <canvas id="move-canvas">
          <!-- Search Moves Start -->
          <div class="row">
            <input id="input-moves" type="text" placeholder="Search Moves..." />
          </div>
          <!-- Search Moves End -->
          <!-- Move Box Start -->
          <div class="row">
            <ul id="move-box-ul" class="list-unstyled"></ul>
          </div>
          <!-- Move Box End -->
        </canvas>
      </div>
    </div>
    <!-- Move Column End -->

  </div>


</div>



<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

