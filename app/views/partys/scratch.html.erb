<script>
  
  var canvas = document.getElementById("move-canvas");
  var ctx - canvas.getContext("2d");
  ctx.beginPath();
  ctx.lineWIdth = "2";
  ctx.strokeStyle = "black";
  ctx.moveTo(0,75);
  ctx.lineTo(250,75);
  ctx.stroke();

</script>


